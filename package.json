{
  "name": "@pyxismedia/lib-react",
  "version": "0.0.1",
  "description": "React Typescript Components Library",
  "author": "Petr Juna <petr.juna@pyxis.media>",
  "homepage": "http://pyxis.media",
  "license": "MIT",
  "main": "build/index.js",
  "files": [
    "build/**/*"
  ],
  "scripts": {
    "storybook": "start-storybook -p 9001 -h 0.0.0.0 -c .storybook",
    "storybook:dry-run": "build-storybook -c .storybook -o .storybook/build --dry-run",
    "storybook:build": "build-storybook -c .storybook -o .storybook/build",
    "storybook:serve": "serve .storybook/build",
    "prettier:lint": "prettier --single-quote --check \"src/**/*.{ts,tsx}\"",
    "prettier": "prettier --single-quote --write \"src/**/*.{ts,tsx}\"",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:update": "react-scripts test -u --watch-all=false",
    "eject": "react-scripts eject",
    "release": "semantic-release",
    "commit": "commit",
    "lib": "rm -rf build && npm run lib:css && NODE_ENV=production babel --extensions \".ts,.tsx\" src --out-dir build --ignore \"src/**/story.tsx,src/**/*.spec.ts,src/**/*.spec.tsx,__snapshots__,src/**/*.d.ts\" && npm run lib:declarations",
    "lib:css": "node-sass-chokidar src/themes/animo/theme.scss build/themes/animo/animo.css",
    "lib:declarations": "tsc -p tsconfig.d.json",
    "build": "npm run storybook:build",
    "deploy": "surge --project .storybook/build --domain lib-react.pyxis.media",
    "precommit": "lint-staged",
    "prepush": "npm ci --dry-run && npm test -- --watchAll=false"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.17",
    "@fortawesome/free-solid-svg-icons": "5.8.1",
    "@fortawesome/react-fontawesome": "0.1.4",
    "@fullpage/react-fullpage": "0.1.17",
    "@stripe/react-stripe-js": "1.1.2",
    "@stripe/stripe-js": "1.5.0",
    "@types/react-transition-group": "^4.4.0",
    "bootstrap": "4.4.1",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "classnames": "2.2.6",
    "debug": "4.1.1",
    "faker": "4.1.0",
    "fullpage.js": "3.0.8",
    "lodash": "^4.17.19",
    "node-sass": "^4.14.1",
    "proxyquire": "2.1.0",
    "react": "16.9.0",
    "react-bootstrap": "1.0.0",
    "react-dom": "16.9.0",
    "react-spring": "8.0.27",
    "react-transition-group": "^4.4.1",
    "react-use-gesture": "7.0.15",
    "recompose": "0.30.0",
    "require-context": "1.1.0",
    "semantic-release": "15.13.17",
    "serve": "10.1.2",
    "svg-inline-react": "3.1.0",
    "typescript": "3.6.3"
  },
  "devDependencies": {
    "@babel/cli": "7.6.0",
    "@babel/plugin-transform-react-display-name": "7.8.3",
    "@commitlint/cli": "8.0.0",
