    "@commitlint/prompt-cli": "11.0.0",
    "@etchteam/storybook-addon-status": "^2.1.0",
    "@fortawesome/fontawesome-free": "5.15.1",
    "@storybook/addon-actions": "6.1.11",
    "@storybook/addon-controls": "6.1.11",
