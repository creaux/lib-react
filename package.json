{
  "name": "@pyxismedia/lib-react",
  "version": "0.0.1",
  "description": "React Typescript Components Library",
  "author": "Petr Juna <petr.juna@pyxis.media>",
  "homepage": "http://pyxis.media",
  "license": "MIT",
  "main": "build/index.js",
  "files": [
    "index.ts",
    "src/**/*"
  ],
  "scripts": {
    "storybook": "start-storybook -p 9001 -h 0.0.0.0 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o .storybook/build",
    "storybook:serve": "serve .storybook/build",
    "prettier:lint": "prettier --single-quote --check \"src/**/*.{ts,tsx}\"",
    "prettier": "prettier --single-quote --write \"src/**/*.{ts,tsx}\"",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:update": "react-scripts test -u",
    "eject": "react-scripts eject",
    "release": "semantic-release",
    "commit": "commit",
    "lib": "rm -rf build && npm run build:css && NODE_ENV=production babel --extensions \".ts,.tsx\" src --out-dir build --ignore \"src/**/story.tsx,src/**/*.spec.ts,src/**/*.spec.tsx,__snapshots__,src/**/*.d.ts\"",
    "lib:css": "node-sass-chokidar src/themes/animo/theme.scss build/themes/animo.css",
    "build": "npm run storybook:build",
    "deploy": "surge --project .storybook/build --domain lib-react.pyxis.media"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "bootstrap": "^4.3.1",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "classnames": "^2.2.6",
    "debug": "^4.1.1",
    "faker": "^4.1.0",
    "lodash": "^4.17.11",
    "node-sass": "^4.12.0",
    "proxyquire": "^2.1.0",
    "react": "^16.8.6",
    "react-bootstrap": "^1.0.0-beta.5",
    "react-dom": "^16.8.6",
    "recompose": "^0.30.0",
    "require-context": "^1.1.0",
    "semantic-release": "^15.13.17",
    "serve": "^10.1.2",
    "svg-inline-react": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@commitlint/prompt-cli": "^8.0.0",
    "@fortawesome/fontawesome-free": "^5.8.1",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-info": "^5.0.11",
    "@storybook/addon-knobs": "^5.0.11",
    "@storybook/addon-options": "^5.0.11",
    "@storybook/addon-storyshots": "^5.0.11",
    "@storybook/core-events": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "@types/chai-enzyme": "^0.6.6",
    "@types/classnames": "^2.2.7",
    "@types/debug": "^4.1.2",
    "@types/faker": "^4.1.5",
    "@types/jest": "24.0.15",
    "@types/lodash": "^4.14.119",
    "@types/node": "12.0.0",
    "@types/react": "16.8.17",
    "@types/react-bootstrap": "^0.32.17",
    "@types/react-dom": "16.8.4",
    "@types/react-transition-group": "^2.0.16",
    "@types/recompose": "^0.30.5",
    "@types/sinon-chai": "^3.2.2",
    "@types/storybook-readme": "^4.0.0",
    "@types/storybook__addon-actions": "3.4.2",
    "@types/storybook__addon-info": "^4.1.0",
    "@types/storybook__addon-knobs": "^4.0.1",
    "@types/storybook__addon-storyshots": "^3.4.7",
    "@types/storybook__react": "^4.0.1",
    "chai": "^4.2.0",
    "chai-enzyme": "^1.0.0-beta.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "husky": "2.7.0",
    "lint-staged": "8.2.1",
    "node-sass-chokidar": "^1.3.5",
    "prettier": "1.18.2",
    "react-scripts": "3.0.1",
    "react-test-renderer": "^16.8.3",
    "sinon": "^7.1.1",
    "sinon-chai": "^3.3.0",
    "storybook-readme": "^5.0.2",
    "surge": "^0.21.3",
    "typescript": "3.4.5"
  },
  "engines": {
    "node": "10.15.3",
    "npm": "6.4.1"
  },
  "lint-staged": {
    "{src}/**/*.ts*": [
      "npm run prettier",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "CI=true npm test && npm run build && npm run lib",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/creaux/lib-react.git"
  },
  "bugs": {
    "url": "https://github.com/creaux/lib-react/issues"
  }
}
